{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 550,
    "height": 400,
    "data": {
        "url": "https://raw.githubusercontent.com/LeeZhenYan/FIT3179/main/Petrol%20Dataset%20June%2020%202022%20-%20Cleaned.csv"
        },
    "config": {"view": {"stroke": null}},
    "mark": "boxplot",
    "background": "lightgray",
    "encoding": {
      "x": {"field": "Continent", "type": "nominal","axis": {"labelAngle": 45}},
      "color": {"field": "Continent", "type": "nominal", "legend": null},
      "y": {
        "field": "Yearly Gallons Per Capita",
        "type": "quantitative",
        "scale": {"zero": false},
        "axis": {"grid": false, "ticks": false}
      },
      "size": {"value": 60}
    }
  }
